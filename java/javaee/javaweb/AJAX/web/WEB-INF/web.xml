<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>yanZhengServlet</servlet-name>
        <servlet-class>com.javasm.control.YanZhengServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>yanZhengServlet</servlet-name>
        <url-pattern>/yan.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>com.javasm.control.RegServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/reg.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>provinceServlet</servlet-name>
        <servlet-class>com.javapractice.control.ProvinceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>provinceServlet</servlet-name>
        <url-pattern>/showPro.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>cityServlet</servlet-name>
        <servlet-class>com.javapractice.control.CityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cityServlet</servlet-name>
        <url-pattern>/showCity.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>areaServlet</servlet-name>
        <servlet-class>com.javapractice.control.AreaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>areaServlet</servlet-name>
        <url-pattern>/showArea.do</url-pattern>
    </servlet-mapping>
</web-app>